import matplotlib
import matplotlib.pyplot as plt
import pandas as pd



vardata= pd.read_csv('plink.eigenvec',delimiter=r"\s+",names=['Drop','Sample name','PC1','PC2','PC3','PC4','PC5','PC6','PC7','PC8','PC9','PC10'])
vardata=vardata.drop(['Drop'], axis=1)





# Plotting barchart
plt.scatter(vardata['PC1'], vardata['PC2'])
  
# Saving the figure.
plt.savefig("output.jpg")
